{
  "name": "hibob-integration-mock-api",
  "version": "0.0.1",
  "engines": {
    "node": "20.8.0",
    "npm": "10.1.0",
    "os": ["darwin", "linux"]
  },
  "bin": {
    "hibob-integration-mock-api": "bin/hibob-integration-mock-api.js"
  },
  "scripts": {
    "auth": "aws sso login",
    "dev:aws": "cdk watch",
    "dev:local": "cdk synth --no-staging && sam local start-api -t ./cdk.out/HibobIntegrationMockApiStack.template.json",
    "lint": "concurrently -c auto 'npm:lint:*'",
    "lint:es": "eslint --max-warnings 0 bin lambda lib test tsconfig.json package.json cdk.json",
    "lint:tsc": "tsc --noEmit",
    "test": "tsx --test ./**/*.test.ts ./*/**/*.test.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@types/aws-lambda": "^8.10.125",
    "@types/eslint": "^8.44.6",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.200",
    "@types/node": "20.7.1",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "aws-cdk": "2.102.0",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.5",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsx": "^3.14.0",
    "type-fest": "^4.5.0",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.102.0",
    "chalk": "^5.3.0",
    "constructs": "^10.0.0",
    "lodash": "^4.17.21",
    "source-map-support": "^0.5.21"
  }
}
