{
  "name": "hibob-integration-mock-api",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": "20.8.0",
    "npm": "10.1.0",
    "os": [
      "darwin",
      "linux"
    ]
  },
  "bin": {
    "hibob-integration-mock-api": "bin/synth.ts"
  },
  "scripts": {
    "auth": "aws sso login",
    "dev:aws": "cdk watch",
    "dev:local": "cdk synth --no-staging && sam local start-api -t ./cdk.out/HibobIntegrationMockApiStack.template.json",
    "generate:mock-data": "tsx ./test/generate-mock-data.ts",
    "init": "concurrently -c auto --timings 'npm:init:*'",
    "init:vs-code": "rsync -avuP vscode-config/ .vscode",
    "init:git-hooks": "cd \"$npm_config_local_prefix/.git/hooks\"; for hook in \"$(find ../../git-hooks -type f)\"; do ln -s\"$([ \"$HOOK_OVERWRITE\" ] && printf 'f')\" \"$hook\" \"$(basename \"$hook\")\"; done",
    "lint:es": "eslint --max-warnings 0 bin lambda lib test tsconfig.json package.json cdk.json",
    "lint:tsc": "tsc --noEmit",
    "postinstall": "HOOK_OVERWRITE=true npm run init",
    "test": "SILENCE_MOCK_DATA=true concurrently --kill-others-on-fail --group --raw --timings 'npm:test:*' 'npm:lint:*' 'npm:generate:mock-data'",
    "test:system": "tsx --test ./**/*.test.ts",
    "test:unit": "tsx --test ./*/**/*.test.ts",
    "test:synth": "bin/synth.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@types/aws-lambda": "^8.10.125",
    "@types/eslint": "^8.44.6",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.200",
    "@types/node": "20.7.1",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "aws-cdk": "^2.102.0",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.5",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsx": "^3.14.0",
    "type-fest": "^4.5.0",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.102.0",
    "chalk": "^5.3.0",
    "constructs": "^10.0.0",
    "dotenv": "^16.3.1",
    "lodash": "^4.17.21",
    "source-map-support": "^0.5.21",
    "status-code-enum": "^1.0.0"
  }
}
